{
  "name": "dush-better-use",
  "version": "0.1.0",
  "description": "Adds support for named plugins and better error handling, by overriding the default `.use` method",
  "repository": "tunnckoCore/dush-better-use",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard index.js test.js --fix --verbose",
    "test": "npm-run-all -s lint test:*",
    "test:api": "nyc --reporter lcov node test.js",
    "test:report": "nyc report",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "git": "npm-run-all -s git:*",
    "git:add": "git add --all",
    "git:cz": "git-cz",
    "commit": "npm-run-all -s test git"
  },
  "dependencies": {
    "get-fn-name": "^1.0.0",
    "minibase-is-registered": "^1.1.5",
    "try-catch-callback": "^2.0.2"
  },
  "devDependencies": {
    "commitizen": "~2.7.0",
    "cz-conventional-changelog": "1.1.5",
    "dush": "^3.0.2",
    "mukla": "^0.4.9",
    "npm-run-all": "~3.1.2",
    "nyc": "^10.2.0",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [],
      "highlight": "dush"
    },
    "lint": {
      "reflinks": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  }
}
